package pokerPackage;

public class CombinationsTester {

	public static void main(String[] args) {
		
		int[] CS = new int[12];
		
//		Player One Cards
		CS[0] = 37;
		CS[1] = 51;
		
//		Player Two Cards
		CS[2] = 38;
		CS[3] = 8;
		
//		Player Three Cards
		CS[4] = 100;
		CS[5] = 100;
		
//		Player Four Cards
		CS[6] = 100;
		CS[7] = 100;
		
//		Flop
		CS[8] = 100;
		CS[9] = 100;
		CS[10] = 100;
		
//		Turn
		CS[11] = 100;
		
		Combinations C = new Combinations(CS);
		C.Calculate();
		C.printResults(false);
		
	}

}
