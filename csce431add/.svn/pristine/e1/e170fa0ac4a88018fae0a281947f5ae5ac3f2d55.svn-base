using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace BUKI
{
    public class RV : Vehicle
    {
        private int m_mpg = 0;
        private int m_squareFeet = 0;

        public int MPG 
        { 
            get { return m_mpg; }
            set { m_mpg = value; }
        }

        public int SquareFeet
        {
            get { return m_squareFeet; }
            set { m_squareFeet = value; }
        }
    }
}
