using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using BUKI;

namespace BUKI
{
    public class Vehicle
    {
        private string m_make;
        private string m_model;
        private int m_year = 0;
        private string m_color;
        private string m_displayPhoto;
        private int m_id = 0;
        public Vehicle(){}
        public Vehicle(int year, int id)
        {
            m_id = id;
            m_year = year;
        }
        public string Make
        {
            get { return m_make; }
            set { m_make = value; }
        }
        public string Display_Photo
        {
            get { return m_displayPhoto; }
            set { m_displayPhoto = value; }
        }
        public string Model
        {
            get { return m_model; }
            set { m_model = value; }
        }
        public int Year
        {
            get { return m_year; }
            set { m_year = value; }
        }
        public int ID
        {
            get { return m_id; }
            set { m_id = value; }
        }
        public string Color
        {
            get { return m_color; }
            set { m_color = value; }
        }
    }
}
