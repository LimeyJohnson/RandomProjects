using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

/// <summary>
/// Summary description for Request
/// </summary>
public class Request
{
    private int m_id;
    private int m_dest_dealership;
    private string m_vin;
    private DateTime m_request_date;
    private int m_approver;
    private bool m_approved;
    public Request(int id, int dest_dealership, string vin, DateTime request_date, int approver, bool approved)
    {
        m_id = id;
        m_dest_dealership = dest_dealership;
        m_vin = vin;
        m_request_date = request_date;
        m_approver = approver;
        m_approved = approved;
    }
    public int ID
    {
        get { return m_id; }
    }
    public int DestinationDealership
    {
        get { return m_dest_dealership; }
    }
    public string VIN
    {
        get { return m_vin; }

    }
    public DateTime RequestDate
    {
        get { return m_request_date; }
    }
    public int Approver
    {
        get { return m_approver; }
    }
    public bool Approved
    {
        get { return m_approved; }
    }
}
